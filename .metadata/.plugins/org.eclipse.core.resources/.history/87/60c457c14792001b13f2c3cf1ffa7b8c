
public class Conta {
	
	//atributos
	
	double saldo;
	int agencia;
	int numero;
	String titular;
	
	
	//metodo deposita
	public void deposita(double valor){
		this.saldo =+ valor;
		
	}
	
	//metodo saca
	public boolean saca(double valor) {
		if(this.saldo >= valor) {
			this.saldo -= valor;
			return true;
		}else {
			return false;
		}
	}

}
