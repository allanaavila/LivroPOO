
public class TestaMetodo {

	public static void main(String[] args) {
		
		Conta conta = new Conta();
		
		
		conta.saldo =  100;
		
		conta.deposita(50);
		
		System.out.println("Saldo da Conta do Paulo: " + conta.saldo);

		
		conta.saca(20);
		
		System.out.println("Novo saldo da conta do Paulo: " + conta.saldo);
	}

}
